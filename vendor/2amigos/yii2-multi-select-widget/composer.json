{
    "name": "2amigos/yii2-multi-select-widget",
    "description": "Multiselect Bootstrap widget for Yii2.",
    "keywords": ["yii", "extension", "widget", "multiselect"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Antonio Ramirez",
            "email": "ramirez.cobos@gmail.com",
            "homepage": "http://2amigos.us"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/bootstrap-multiselect": "0.9.*",
        "bower-asset/multiselect": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\multiselect\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
